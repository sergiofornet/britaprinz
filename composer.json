{
	"name": "bettysymington/britaprinzarte",
	"description": "WordPress project for Brita Prinz Arte",
	"config": {
		"platform": {
			"php": "7.3"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		},
		{
            "type": "vcs",
            "url": "https://github.com/afragen/git-updater"
        }
	],
	"require": {
		"htmlburger/carbon-fields": "^3.3",
		"afragen/git-updater": "^9",
		"composer/installers": "^1",	
		"htmlburger/carbon-fields-yoast": "dev-master",
		"wp-coding-standards/wpcs": "dev-develop"
	},
	"minimum-stability": "dev",
	"extra": {
		"installer-paths": {
			"wp-content/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"wp-content/themes/{$name}/": [
				"type:wordpress-theme"
			]
		}
	}
}
